<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <SignAssembly>true</SignAssembly>
    <TargetFrameworks>net461;netstandard2.0;net472;net6.0;net8.0;net9.0</TargetFrameworks>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <NoWarn>$(NoWarn);CS1591</NoWarn>
    <Copyright>2025 - $([System.DateTime]::Now.Year) Marc Gravell</Copyright>
  </PropertyGroup>

  <ItemGroup>
    <ProjectReference Include="..\RESP.Core\RESP.Core.csproj" />
  </ItemGroup>

  <ItemGroup>
    <Compile Update="RespOperationT.cs">
      <DependentUpon>RespOperation.cs</DependentUpon>
    </Compile>
    <Compile Update="IRespParser_Typed_Stateful.cs">
      <DependentUpon>IRespParser_Typed.cs</DependentUpon>
    </Compile>
    <Compile Update="Internal\IRespMessage.cs">
      <DependentUpon>RespMessage.cs</DependentUpon>
    </Compile>
    <Compile Update="Internal\RespMessageBase_Typed_Stateful.cs">
      <DependentUpon>RespMessageBaseT.cs</DependentUpon>
    </Compile>
    <Compile Update="Internal\RespMessageBase_Typed.cs">
      <DependentUpon>RespMessageBaseT.cs</DependentUpon>
    </Compile>
  </ItemGroup>

</Project>
